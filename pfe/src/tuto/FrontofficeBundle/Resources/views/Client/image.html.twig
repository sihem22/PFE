{%for user in user%}
    {% if user.path is null %}
        {% if is_granted('ROLE_PROF') %}
             <img class="thumbnail" style="width: 200px;height: 200px;"  alt="" src="{{ asset('front/images/images (3)_1.jpg') }}"> {%endif%}
             {% if is_granted('ROLE_CLIENT') %}
   <img class="thumbnail" style="width: 200px;height: 200px;"  alt="" src="{{ asset('front/images/images (4)_1.jpg') }}">{%endif%}
   {% if is_granted('ROLE_ADMIN') %}
   <img class="thumbnail" style="width: 200px;height: 200px;"  alt="" src="{{ asset('front/images/images (10).jpg') }}">{%endif%}
    {%else%}
 <img class="thumbnail" src="{{ asset(user.assetPath)}}" style="width: 300px;height: 350px;" >{%endif%}
 {%endfor%}