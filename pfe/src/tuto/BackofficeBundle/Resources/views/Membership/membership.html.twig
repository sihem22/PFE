
{% extends ":back:base.html.twig" %}
{% block title %}Membership{% endblock  %}
{% block body %}
    <div class="container">
        <div class="row">
           <div class="row">
            <div class="col-md-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="clip-home-2"></i>
                         <a href="{{path('frontoffice_homepage')}}">
                            Front Office
                        </a>
                    </li>
                    <li class="active">
                        <i><b>  Back Office </b> </i>
                    </li>
                    
                </ol>
            </div>
        </div>
        </div>
        {% for label, flashes in app.session.flashbag.all %}
            {% for flash in flashes %}
                <div class="alert alert-success">
                    <button class="close" data-dismiss="alert">
                        Ã—
                    </button>
                    <i class="fa fa-check-circle"></i>
                    <strong>{{ label|title }} !!!</strong> :  {{ flash }}

                </div>
            {% endfor %}
        {% endfor %}
       
        
        
        
        <div class="row">
            <div class="col-md-12">
                <!-- start: DYNAMIC TABLE PANEL -->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-external-link-square"></i>
                        Liste des Membership
                        <div class="panel-tools">
                            <a class="btn btn-xs btn-link panel-collapse collapses" href="#"> </a>
                            <a class="btn btn-xs btn-link panel-expand" href="#"> <i class="fa fa-resize-full"></i> </a>
                            
                        </div>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped table-bordered table-hover table-full-width" id="sample_1">
                            <thead>
                                <tr>
                           
                                     <th class="center">DateDebut</th>
                                     
                                       <th class="center">CreerPar</th>
                                       <th class="center">TotalPoints</th>
                                       <th class="center">User</th>
                                       <th class="center">Etat</th>
                                       <th class="center">TypeAbonnement</th>
                                     
                                 
                                    
                                </tr>
                            </thead>
                            <tbody>
                            {% for membership in membership %}
                               <tr>
    
                                 <td class="center">{{membership.dateDebut}}</td>
                                 <td class="center">{{membership.dateFin}}</td>
                                 <td class="center"> {{dmembership.creerPar}} </td>
                                  <td class="center"> {{membership.totalPoints}} </td>
                                 <td class="center"> {{membership.user}} </td>
                                 <td class="center"> {{membership.etat}} </td>
                                 <td class="center"> {{membership.typeAbonnement}} </td>
     
                                 </tr>
                               {%endfor%}
                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- end: DYNAMIC TABLE PANEL -->
            </div>
        </div>
    </div>
        
    {% endblock  %}

