{% extends ":back:base.html.twig" %}
{% block title %} Contact {% endblock  %}
{% block body %}
    <div class="container">
        <div class="row">
           <div class="row">
            <div class="col-md-12">
                <ol class="breadcrumb">
                    <li>
                        <i class="clip-home-2"></i>
                         <a href="{{path('frontoffice_homepage')}}">
                            Front Office
                        </a>
                    </li>
                    <li class="active">
                        <i><b>  Back Office </b> </i>
                    </li>
                    
                </ol>
            </div>
        </div>
        </div>
        {% include ":back:alert.html.twig" %}




        <div class="row">
            <div class="col-md-12">
                <!-- start: DYNAMIC TABLE PANEL -->
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <i class="fa fa-external-link-square"></i>
                        Liste des Contactes
                        <div class="panel-tools">
                            <a class="btn btn-xs btn-link panel-collapse collapses" href="#"> </a>
                            <a class="btn btn-xs btn-link panel-expand" href="#"> <i class="fa fa-resize-full"></i> </a>
                            <a href="{{path('tuto_categorie_ajout')}}" > <span class="glyphicon glyphicon-plus" aria-hidden="true" > </span> </a>
                        </div>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped table-bordered table-hover table-full-width" id="sample_1">
                            <thead>
                                <tr>
                                    <th class="center">Nom</th>
                                   
                                    <th class="center"> E-mail </th>
                                    
                                    <th class="center"> telephone</th>
                                     <th class="center"> Commentaire</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                {% for contact in contacts %}
                                    <tr>
                                        <td class="center"> {{contact.nom}}</td>
                                        
                                        <td class="center">{{contact.email}}</td>

                                        <td class="center">{{contact.telephone}}</td>

                                         <td class="center">{{contact.commentaire}}</td>
                                        
                                    </tr>
                                {%endfor%}
                            </tbody>
                        </table>
                    </div>
                </div>
                <!-- end: DYNAMIC TABLE PANEL -->
            </div>
        </div>
    </div>
    {% endblock  %}
